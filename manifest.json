{
    "manifest_version": 2,
    "name": "AIDPASS",
    "version": "1.0",
    "browser_action": {
        "default_title": "AIDPASS",
        "default_popup": "src/index.html",
        "default_icon": "src/static/icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "src/js/index.js"
            ]
        }
    ]
}